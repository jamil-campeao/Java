import java.awt.Insets;
import java.awt.Point;

import javax.swing.JFrame;

public class classesUtilitarias {

	public static void main(String[] args) {
JFrame frame = new JFrame();
		
		Insets ins = new Insets(20,5,5,5);
		System.out.println(ins);
		
		 // Get the insets of a JFrame
		Insets insFrame = frame.getInsets();
		
		int top    = insFrame.top;
		int left   = insFrame.left;
		int bottom = insFrame.bottom;
		int right  = insFrame.right;
		
		System.out.println(insFrame);

	}

}
